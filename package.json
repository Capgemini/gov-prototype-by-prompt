        "@opentelemetry/sdk-trace-node": "~2.5.0",
        "ace-builds": "~1.43.6",
        "ace-linters": "~2.0.0",
        "bcryptjs": "~3.0.3",
        "body-parser": "~2.2.2",
        "client-zip": "~2.5.0",
        "compression": "~1.8.1",
        "dotenv": "~17.3.1",
        "express": "~5.2.1",
        "express-rate-limit": "~8.2.1",
        "express-session": "~1.19.0",
        "express-validator": "~7.3.1",
        "govuk-frontend": "~6.0.0",
        "govuk-markdown": "~0.8.0",
        "helmet": "~8.1.0",
        "hmrc-frontend": "~6.115.0",
        "html-format": "~1.1.7",
        "jsonschema": "~1.5.0",
        "marked": "~17.0.3",
        "mermaid": "~11.12.3",
        "moment": "~2.30.1",
        "mongoose": "~9.2.1",
