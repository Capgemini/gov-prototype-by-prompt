{
    "name": "gov-prototype-by-prompt",
    "version": "1.0.0",
    "description": "Rapidly create, test, and refine GOV.UK Design System form prototypes from a simple description using GenAI.",
    "keywords": [
        "express",
        "generative-ai",
        "govuk",
        "nodejs",
        "nunjucks",
        "prototyping",
        "typescript",
        "user-research",
        "ux-design"
    ],
    "homepage": "https://github.com/Capgemini/gov-prototype-by-prompt",
    "bugs": {
        "url": "https://github.com/Capgemini/gov-prototype-by-prompt/issues"
    },
    "license": "MIT",
    "author": "Digital Excellence Lab, Capgemini Invent UK",
    "contributors": [
        "Bilal Mahmood",
        "Chris Currie",
        "Christopher Menon",
        "Dragos-Andrei Ilies",
        "George Herbert",
        "Jag Gill",
        "Jake Readman",
        "Rahmat Junaid",
        "Rosie Kelly",
        "Ryan Smith",
        "Steven Tran",
        "Toba Odumosu"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Capgemini/gov-prototype-by-prompt.git"
    },
    "engines": {
        "node": ">=20.0.0 <21.0.0"
    },
    "scripts": {
        "start": "npx --yes tsx --require ./src/instrumentation.ts server.ts",
        "dev": "tsx watch ./server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:db": "tsx ./src/database/test-connection.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "db:start": "docker-compose up -d",
        "db:stop": "docker-compose down",
        "db:reset": "docker-compose down -v && docker-compose up -d"
    },
    "dependencies": {
        "@azure/monitor-opentelemetry-exporter": "~1.0.0-beta.32",
        "@opentelemetry/api": "~1.9.0",
        "@opentelemetry/auto-instrumentations-node": "~0.67.0",
        "@opentelemetry/instrumentation-express": "~0.57.0",
        "@opentelemetry/instrumentation-http": "~0.208.0",
        "@opentelemetry/sdk-metrics": "~2.2.0",
        "@opentelemetry/sdk-node": "~0.208.0",
        "@opentelemetry/sdk-trace-node": "~2.2.0",
        "ace-builds": "~1.43.2",
        "bcryptjs": "~3.0.2",
        "body-parser": "~2.2.0",
        "client-zip": "~2.5.0",
        "dotenv": "~17.2.1",
        "express": "~5.1.0",
        "express-rate-limit": "~8.2.1",
        "express-session": "~1.18.2",
        "express-validator": "~7.3.0",
        "govuk-frontend": "~5.13.0",
        "govuk-markdown": "~0.8.0",
        "helmet": "~8.1.0",
        "hmrc-frontend": "~6.103.0",
        "html-format": "~1.1.7",
        "jsonschema": "~1.5.0",
        "marked": "~17.0.0",
        "moment": "~2.30.1",
        "mongoose": "~8.19.1",
        "nunjucks": "~3.2.4",
        "openai": "~6.9.1",
        "uuid": "~13.0.0",
        "zod": "~3.25.76"
    },
    "devDependencies": {
        "@cspell/dict-en-gb": "~5.0.11",
        "@cspell/eslint-plugin": "~9.3.0",
        "@eslint/js": "~9.39.1",
        "@types/body-parser": "~1.19.6",
        "@types/express": "~5.0.3",
        "@types/express-session": "~1.18.2",
        "@types/jest": "~30.0.0",
        "@types/node": "^20.0.0",
        "@types/nunjucks": "~3.2.6",
        "@types/supertest": "~6.0.3",
        "cspell": "~9.3.0",
        "eslint": "~9.39.1",
        "eslint-config-prettier": "~10.1.8",
        "eslint-plugin-only-warn": "~1.1.0",
        "eslint-plugin-perfectionist": "~4.15.0",
        "jest": "~30.2.0",
        "jiti": "~2.6.0",
        "mongodb-memory-server": "~10.3.0",
        "node-mocks-http": "~1.17.2",
        "nodemon": "~3.1.10",
        "prettier": "~3.6.2",
        "supertest": "~7.1.4",
        "ts-jest": "~29.4.1",
        "ts-node": "~10.9.2",
        "tsx": "~4.20.3",
        "typescript": "~5.9.2",
        "typescript-eslint": "~8.48.0"
