        "@cspell/eslint-plugin": "~9.6.4",
        "@eslint/js": "~9.39.2",
        "@types/body-parser": "~1.19.6",
        "@types/compression": "~1.8.1",
        "@types/express": "~5.0.6",
        "@types/express-session": "~1.18.2",
        "@types/jest": "~30.0.0",
        "@types/node": "^20.19.0",
        "@types/nunjucks": "~3.2.6",
        "@types/supertest": "~6.0.3",
        "cspell": "~9.6.4",
